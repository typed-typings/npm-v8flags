{
  "name": "@types/v8flags",
  "version": "2.0.0",
  "description": "Types for https://github.com/js-cli/js-v8flags",
  "typings": "index.d.ts",
  "scripts": {
    "lint": "tslint \"**/*.ts\" -e \"node_modules/**\" -e \"typings/**\" -e bundle.d.ts",
    "bundle": "typings bundle -o bundle.d.ts",
    "exec": "tsc && ts-node test.ts",
    "test": "npm run lint && npm run bundle && npm run exec",
    "prepublish": "typings install",
    "postpublish": "greenkeeper-postpublish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/types/npm-v8flags.git"
  },
  "author": {
    "name": "Blake Embrey",
    "email": "hello@blakeembrey.com",
    "url": "http://blakeembrey.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/types/npm-v8flags/issues"
  },
  "homepage": "https://github.com/types/npm-v8flags#readme",
  "devDependencies": {
    "greenkeeper-postpublish": "^1.0.1",
    "ts-node": "^3.0.3",
    "tslint": "^5.2.0",
    "tslint-config-typings": "^0.3.1",
    "typescript": "^2.0.3",
    "typings": "^2.1.1",
    "v8flags": "^2.0.11"
  }
}
